.mainmenu {
    margin:0px;
    padding:0px;
    display:table;
    width:955px;
    border-collapse: collapse;
    height:18px;
}

.mainmenu ul{
    padding:0px;
    list-style:none;
    display:table-row-group;
}

.mainmenu li{
    display:table-cell;
    border: 0em solid #dcdce9;
    vertical-align: middle;
    letter-spacing:.01em;
    
}

.mainmenu li a{
    padding:14px 6px 10px 4px;
    display:block;
    color: #ffffff;
    text-decoration: none;
    text-align: center;
    font-family:Fjalla One;
    font-size:18px;
    font-weight:normal;
    margin-left:5px;
    margin-right:5px; 
}
 
.mainmenu ul li a:hover, 
 body#home #nav-home,
 body#about #nav-about,
 body#reliability #nav-reliability,
 body#lktapps #nav-lktapps,
 body#cal #nav-cal,
 body#contact #nav-contact
 { 
	padding:14px 6px 10px 4px;
    display:block;
    color: #1a4d7c;
    background: #8ec102;
    text-decoration: none;
    text-align: center;
    font-family:Fjalla One;
    font-size:18px;
    font-weight:normal;
    margin-left:5px;
    margin-right:5px; 
	 
 }
 


